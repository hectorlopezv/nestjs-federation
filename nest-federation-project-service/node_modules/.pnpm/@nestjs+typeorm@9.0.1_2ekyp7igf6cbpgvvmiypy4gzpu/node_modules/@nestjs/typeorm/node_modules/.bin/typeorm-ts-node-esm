#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hvlopez/nest-federation-project-service/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hvlopez/nest-federation-project-service/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.10_pa4ea3wjef6s4gsskhbyci3cte/node_modules/typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.10_pa4ea3wjef6s4gsskhbyci3cte/node_modules/typeorm/cli-ts-node-esm.js" "$@"
fi
